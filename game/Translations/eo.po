#: ../Scenes/Game/CameraController.tscn:574
#: ../Scenes/Game/CameraController.tscn:641
#: ../Scenes/Game/CameraController.tscn:695 ../Scenes/OptionsMenu.tscn:1309
msgid "OK"
msgstr "Bone"

#: ../Scenes/OptionsMenu.tscn:1354
msgid "Please press a key or button to bind it..."
msgstr "Bonvolu premi klavon aŭ butonon por bindi ĝin..."

#: ../Scenes/OptionsMenu.tscn:1374
msgid "Cancel"
msgstr "Nuligi"

#: ../Scenes/OptionsMenu.tscn:1381
msgid "Keep video settings?"
msgstr "Ĉu konservi la video-agordojn?"

#: ../Scenes/OptionsMenu.tscn:1389
msgid "Reimporting Assets"
msgstr "Reimportado de havaĵoj"

#: ../Scenes/OptionsMenu.tscn:1407
msgid "Warning!"
msgstr "Averto!"

#: ../Scenes/RandomMusicPlayer.tscn:23
msgid "Now Playing:"
msgstr "Nune ludata:"

#: ../Scenes/Game/CameraController.tscn:98
msgid "Deal X cards"
msgstr "Disdoni X kartojn"

#: ../Scenes/Game/CameraController.tscn:128
msgid "Select track"
msgstr "Elekti trakon"

#: ../Scenes/Game/CameraController.tscn:146
msgid "Positional audio"
msgstr "Pozicia sono"

#: ../Scenes/Game/CameraController.tscn:152
msgid "Range:"
msgstr "Amplekso:"

#: ../Scenes/Game/CameraController.tscn:172
msgid "Take X off top"
msgstr "Preni X de la supro"

#: ../Scenes/Game/CameraController.tscn:185
msgid "Take X out"
msgstr "Elpreni X"

